{
  "name": "nevadskiy/laravel-translatable",
  "description": "Laravel package that allows to translate models into different languages",
  "license": "MIT",
  "authors": [
    {
      "name": "Vitalii Nevadskiy",
      "email": "nevadskiy@gmail.com"
    }
  ],
  "require": {
    "laravel/framework": "^6.0|^7.0",
    "php": "^7.2",
    "stichoza/google-translate-php": "^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "orchestra/testbench": "^4.0",
    "mockery/mockery": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "Nevadskiy\\Translatable\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nevadskiy\\Translatable\\Tests\\": "tests/"
    }
  }
}
