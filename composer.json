{
  "name": "nevadskiy/laravel-translatable",
  "description": "Laravel package that allows to translate models into different languages",
  "license": "MIT",
  "keywords": ["translations", "localization", "laravel"],
  "support": {
    "issues": "https://github.com/nevadskiy/laravel-translatable/issues",
    "docs": "https://github.com/nevadskiy/laravel-translatable"
  },
  "authors": [
    {
      "name": "Nevadskiy",
      "email": "nevadskiy@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "laravel/framework": "^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "orchestra/testbench": "^5.0",
    "mockery/mockery": "^1.2",
    "friendsofphp/php-cs-fixer": "^2.16"
  },
  "autoload": {
    "psr-4": {
      "Nevadskiy\\Translatable\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nevadskiy\\Translatable\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Nevadskiy\\Translatable\\TranslatableServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "phpunit --colors=always",
    "fix": "php-cs-fixer fix"
  }
}
